    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "note-processor:src/index.handler (nodejs14.x)",
            "invokeTarget": {
                "target": "code",
                "projectRoot": "./.build",
                "lambdaHandler": "index.handler"
            },
            "lambda": {
                "runtime": "nodejs14.x",
                "payload": {
                    "json": {
                        "noteText": "foo",
                        "hasLocation": "true",
                        "latitude": "1.2345",
                        "longitude": "1.2345",
                        "noteDate": "2021-7-29 14:04:05"
                    }
                },
                "environmentVariables": {},
            }
        }
    ]
}